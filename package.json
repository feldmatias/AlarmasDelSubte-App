    "eslint": "^6.5.1",
    "jest": "^25.0.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "react-native-testing-library": "^1.12.0",
    "react-test-renderer": "16.9.0",
    "ts-jest": "^25.0.0",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^4.2.0",
    "typescript": "^3.7.3"
  },
  "jest": {
    "preset": "react-native",
