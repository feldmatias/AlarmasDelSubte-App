    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.jest.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testRegex": "/tests/.*.(ts|tsx|js)$",
    "setupFilesAfterEnv": ["./tests/setup.tsx"],
    "modulePathIgnorePatterns": ["./tests/setup.tsx"]
  }
